<h1>
  Descriptive Statistics Results
</h1>

<dl class="dl-horizontal">
  <dt>
    Numbers
  </dt>
  <dd class="numbers"> <%= @numbers %> 
    
  </dd>

  <dt>
    Sorted Numbers
  </dt>
  <dd class="sorted_numbers">  <%= @sorted_numbers %> 

  </dd>

  <dt>
    Count
  </dt>
  <dd class="count"> <%= @count %> 

  </dd>

  <dt>
    Minimum
  </dt>
  <dd class="minimum"> <%= @min %> 

  </dd>

  <dt>
    Maximum
  </dt>
  <dd class="maximum"> <%= @max %> 
  </dd>

  <dt>
    Range
  </dt>
  <dd class="range">  <%= @range %> 
    
  </dd>

  <dt>
    Median
  </dt>
  <dd class="median">
   <%= @median_final %> 
  </dd>

  <dt>
    Sum
  </dt>
  <dd class="sum"><%= @sum %> 
  
  </dd>

  <dt>
    Mean
  </dt>
  <dd class="mean"> <%= @mean %> 
   
  </dd>

  <dt>
    Variance
  </dt>
  <dd class="variance"> <%= @variance %> 
  
  </dd>

  <dt>
    Standard Deviation
  </dt>
  <dd class="standard_deviation"> <%= @standard_deviation %> 

  </dd>

  <dt>
    Mode
  </dt>
  <dd class="mode"> <%= @mode_final %> 
 
  </dd>
</dl>

<a href="/descriptive_stats/new">
  Calculate more descriptive statistics
</a>